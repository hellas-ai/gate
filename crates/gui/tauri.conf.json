{
  "productName": "Gate",
  "version": "0.1.0",
  "identifier": "ai.hellas.gate",
  "build": {
    "frontendDist": "../frontend-tauri/dist",
    "devUrl": "http://localhost:8082",
    "beforeDevCommand": "sh -c 'cd frontend-tauri && trunk serve'",
    "beforeBuildCommand": "echo 'DEBUG: PWD before cd:' && pwd && echo 'DEBUG: Contents of parent dir:' && ls -la ../ && echo 'DEBUG: Attempting cd to ../frontend-daemon' && cd ../frontend-daemon && pwd && trunk build --release && cd ../frontend-tauri && pwd && trunk build --release"
  },
  "app": {
    "windows": [
      {
        "title": "Hellas Gate",
        "width": 400,
        "height": 600,
        "resizable": false
      }
    ],
    "security": {
      "csp": null,
      "dangerousDisableAssetCspModification": true
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": {
      "resources/frontend-daemon/": "frontend-daemon/"
    },
    "macOS": {
      "entitlements": "./entitlements.plist",
      "hardenedRuntime": true
    }
  },
  "plugins": {}
}