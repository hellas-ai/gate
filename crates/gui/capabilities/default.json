{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    {
      "identifier": "opener:allow-open-url",
      "allow": [
        {
          "url": "https://*.private.hellas.ai"
        },
        {
          "url": "https://*.private.hellas.ai/*"
        }
      ]
    },
    "opener:allow-open-path"
  ]
}